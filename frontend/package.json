		"react-i18next": "^12.1.1",
		"react-icons": "^4.7.1",
		"react-is": "^18.2.0",
		"react-redux": "^8.0.5",
		"react-router-dom": "^6.4.5",
		"styled-components": "^5.3.6"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
